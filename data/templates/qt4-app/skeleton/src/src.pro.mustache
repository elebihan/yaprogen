QT      += core gui
SOURCES += \
    main.cpp

HEADERS +=

TARGET = {{executable_name}}

RESOURCES +=

unix {
    isEmpty(PREFIX) {
        PREFIX = /usr
    }
    BINDIR = $$PREFIX/bin
    DATADIR = $$PREFIX/share

    DEFINES += DATADIR=\\\"$$DATADIR\\\" PKGDATADIR=\\\"$$PKGDATADIR\\\"

    target.path = $$BINDIR

    INSTALLS += target
}
